#Type Casting.

import java.util.*;
class TypeCasting{
    
public static void main(String[] args) 
 
  {
        
           int a = 100;
       
           long b = a;
        
           double c = b;
        
           float d =(float)c;

       
           System.out.println("Integer Value: " + a);
       
           System.out.println("Long Value: " + b);
       
           System.out.println("Double Value: " + c);
     
           System.out.println("float Value: " + d);
 
   }

}
